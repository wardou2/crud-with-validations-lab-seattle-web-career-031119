<h2> New Song </h2>
<%= form_for @song do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %><br>
  <%= f.label :release_year %>
  <%= f.number_field :release_year %><br>
  <%= f.label :released %><br>
  <%= f.check_box :released%>
  <%= f.label :genre %>
  <%= f.text_field :genre %><br>
  <%= f.submit %>
<% end %>
